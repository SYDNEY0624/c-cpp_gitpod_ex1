#include <stdio.h>

#include <stdio.h>

int main() {
    int a, b, product;
    a = 41;
    b = 17;

 
    // Calculating product
    product = a + b;

 
    printf("Product = %d", product);
    
     product = a - b;

 
    printf("Product = %d", product);
    
    
     product = a * b;

 
    printf("Product = %d", product);
    
    
     product = a / b;

 
    printf("Product = %d", product);
    
     product = a % b;

 
    printf("Product = %d", product);
    
    
    /*
    ++x
x++
--x
x--

x = 10 
x >> 2
x >> 4
x << 6
    */
    
     ++a; 

 
    printf("Product = %d", a);
    
    
    return 0;
    
    a++; 

 
    printf("Product = %d", a);
    
    
    return 0;
}
