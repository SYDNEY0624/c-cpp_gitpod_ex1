#include<stdio.h>

int main()
{
    int a;
    printf(" please input a \n");
    scanf(" %d",&a);
    if(a == 1)
        printf("monday \n");
    else if(a == 2)
        printf("tuesday \n");
     else if(a == 3)
        printf("wednesday \n");
     else if(a == 4)
        printf("thursday \n");
     else if(a == 5)
        printf("friday \n");
    else if(a == 6)
        printf("saturday \n");
     else if(a == 7)
       printf("sunday \n");
     else if(a == 8)
    
        printf("Hello world");
        
        return 0;
}
